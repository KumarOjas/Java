//Program to add two variables
public class variable
 {   
   public static void main(String[] args)
   { 
       int a=10;
       int b=10;
       int c=a+b;
       System.out.println(c);
    }}
     
 //Program to convert int value to float
 public class variable
 {   
   public static void main(String[] args)
   { 
       int a=10;
       float f=a;
       System.out.println(a);
       System.out.println(f);
    }
}
     
