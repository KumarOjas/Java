

    public class variable
 {   
   public static void main(String[] args)
   {  
      float f=10.5f ;
      int a=(int)f;
      System.out.println(a);
      System.out.println(f);
    }
}
    
    
    public class variable
 {   
   public static void main(String[] args)
   {  
      int a=130;
      byte b=(byte)a;
      System.out.println(a);
      System.out.println(b);
    }
}
     
    
        public class variable
 {   
   public static void main(String[] args)
   {  
      byte a=10;
      byte b=10;
      byte c=(byte)(a+b); 
      System.out.println(c);
    }
}
    
    
